# Production Environment Variables
# Copy this file to .env.production and update with your actual values

VITE_APP_NAME=ReactionSharingPlatform

# Backend API URL (HTTPSを使用)
VITE_API_BASE_URL=https://your-backend-domain.com

# WebSocket Signaling URL (WSSを使用 - HTTPSのWebSocket版)
VITE_SIGNALING_URL=wss://your-backend-domain.com/ws

# Ion-SFU URL
VITE_ION_SFU_URL=https://your-sfu-domain.com

# STUN Servers (カンマ区切り)
VITE_STUN_SERVERS=stun:stun.l.google.com:19302

# Optional settings
VITE_LOG_LEVEL=info
VITE_MAX_PARTICIPANTS=30
VITE_VIDEO_RESOLUTION=360p
VITE_TARGET_FPS=30
